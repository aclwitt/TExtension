{
  "manifest_version": 2,

  "name": "TExtension",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "/src/images/phone_icon.png",
    "default_popup": "/src/html/popup.html"
  },

  "web_accessible_resources": [ 
      "/src/html/*",
      "/src/js/*",
      "/src/style/*",
      "/src/php/*"
  ]
}